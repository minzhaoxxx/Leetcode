class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        
        unordered_map<int,int>map;
        
        for(int i=0;i<nums.size();i++){
            map[nums[i]]=i;
        }
        vector<int>res;
        for(int i=0;i<nums.size();i++){
            if(map.count(target-nums[i])&&i!=map[target-nums[i]]){
                res.push_back(i);
                res.push_back(map[target-nums[i]]);
                break;
            } 
        }
        
        return res;
        
    }
};
